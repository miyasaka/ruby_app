<p id="notice"><%= notice %></p>

<h1>Listing Sample2s</h1>

<table>
  <thead>
    <tr>
      <th>Col1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sample2s.each do |sample2| %>
      <tr>
        <td><%= sample2.col1 %></td>
        <td><%= link_to 'Show', sample2 %></td>
        <td><%= link_to 'Edit', edit_sample2_path(sample2) %></td>
        <td><%= link_to 'Destroy', sample2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sample2', new_sample2_path %>
